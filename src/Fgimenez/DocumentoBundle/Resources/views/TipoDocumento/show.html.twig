{% extends 'layout.html.twig' %}

{% block title %}
    {{ parent() }} - Tipo Soporte
{% endblock %}

{% block page_content %}





    <div class="row">
        <div class="col-md-9">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title">Mostrar Tipo Soporte <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table class="table table-bordered">                    <tr>
                            <td><strong>Id</strong></td>
                            <td> 
                                {{ tipoDocumento.id }}
                            </td>                     
                        </tr>                    <tr>
                            <td><strong>Nombre</strong></td>
                            <td> 
                                {{ tipoDocumento.nombre }}
                            </td>                     
                        </tr>                    <tr>
                            <td><strong>Descripci&oacute;n</strong></td>
                            <td> 
                                {{ tipoDocumento.descripcion }}
                            </td>                     
                        </tr>                    <tr>
                            <td><strong>Estatus</strong></td>
                            <td> 
                                {{ tipoDocumento.status }}
                            </td>                     
                        </tr>                </table>
                </div>
                <!-- /.box-body -->
            </div>
        </div>
    </div>



    <hr/>


    <div class="form-group">
        <a class="btn btn-default" href="{{ path('tipodocumento') }}">
            <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
            Volver al listado
        </a>
        <a class="btn btn-primary" href="{{ path('tipodocumento_edit', { 'id':  tipoDocumento.id }) }}">
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
            Editar
        </a>
        <a class="btn btn-info" href="{{ path('tipodocumento_new') }}">
            <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
            Nuevo
        </a>
        {#<form action="{{ path('tipodocumento_delete', { 'id': tipoDocumento.id }) }}" method="post" style="display: inline-block">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
                <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                Eliminar
            </button>
        </form>#}
    </div>


{% endblock %}
