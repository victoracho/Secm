
    public function testCompleteScenario()
    {
        $client = $this->client;
        $this->logIn();


        // Go to the list view
        $crawler = $client->request('GET', '/admin/{{ route_prefix }}/');
        $this->assertTrue(200 === $client->getResponse()->getStatusCode());

        // Go to the show view
        $crawler = $client->click($crawler->selectLink('show')->link());
        $this->assertTrue(200 === $client->getResponse()->getStatusCode());
    }